{
    "$schema": "https://json.schemastore.org/package",
    "name": "@boundlessdigital/meraki-dashboard-api",
    "version": "0.1.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "/lib"
    ],
    "scripts": {
        "build": "npm run generate-api && tsc",
        "clean": "rm -rf lib",
        "build:lib": "",
        "build:types": "rm -rf ./lib/types/*; mkdir -p lib/types; cp -r types/* lib/types/; echo 'Done'",
        "watch": "tsc --watch",
        "prettier-format": "prettier 'src/**/*.ts' --write",
        "test": "jest",
        "generate-api": "rm -rf ./src/meraki/api; openapi-generator-cli generate -i https://api.meraki.com/api/v1/openapiSpec -g typescript-axios -o ./src/meraki/api",
        "republish": "npm run build && npm publish --access=public"
    },
    "exports": {
        ".": "./lib/index.js"
    },
    "devDependencies": {
        "@jest/globals": "^29.3.1",
        "@openapitools/openapi-generator-cli": "^2.5.2",
        "@types/jest": "^29.2.4",
        "@types/lodash": "^4.14.191",
        "@types/node": "^18.11.12",
        "@types/to-json-schema": "^0.2.1",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.3.1",
        "lodash": "^4.17.21",
        "openapi-types": "^12.1.0",
        "prettier": "2.8.1",
        "ts-config": "^20.10.0",
        "ts-jest": "^29.0.3",
        "tsc-watch": "^6.0.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "axios": "^1.2.1",
        "axios-retry": "^3.3.1",
        "json-schema-to-typescript": "^11.0.2",
        "to-json-schema": "github:boundlessdigital/to-json-schema"
    }
}
